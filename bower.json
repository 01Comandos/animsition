{
  "name": "animsition",
  "description": "a simple and easy jQuery plugin used to add page transition on css animations.",
  "version": "3.1.0",
  "keywords": [
    "css3",
    "js",
    "less",
    "animation",
    "page transition",
    "jquery",
    "pligin"
  ],
  "homepage": "http://git.blivesta.com/animsition",
  "main": [
    "dist/less/**",
    "dist/css/animsition.css",
    "dist/css/animsition.min.css",
    "dist/js/animsition.js",
    "dist/js/animsition.min.js"
  ],
  "ignore": [
    ".*",
    "src",
    "*.yml",
    "*.html",
    "*.json",
    "Gruntfile.js"
  ],
  "dependencies": {
    "cooker": "~0.4.7"
  },
  "exportsOverride": {
    "cooker": {
      "js": "dist/js/dropdown.js",
      "css": "dist/css/",
      "less": "dist/less/"
    }
  }


}