{
  "name": "animsition",
  "description": "A simple and easy jQuery plugin for CSS animated page transitions.",
  "version": "3.3.3",
  "keywords": [
    "css3",
    "js",
    "less",
    "animation",
    "page transition",
    "jquery",
    "pligin"
  ],
  "homepage": "http://git.blivesta.com/animsition",
  "main": [
    "dist/less/**",
    "dist/css/animsition.css",
    "dist/css/animsition.min.css",
    "dist/js/animsition.js",
    "dist/js/animsition.min.js"
  ],
  "ignore": [
    ".*",
    "src",
    "*.yml",
    "*.html",
    "*.json",
    "Gruntfile.js"
  ],
  "devDependencies": {
    "cooker": "~0.4.8"
  },
  "exportsOverride": {
    "cooker": {
      "css": "dist/css/",
      "less": "dist/less/"
    }
  }
}
